	<content><![CDATA[
for (int i = 0; i < s.size(); ++i) {
		sort(s.begin() + i , s.end());
		int64_t posi = i + nth / fact[s.size() - i - 1];
		swap(s[i] , s[posi]);
		nth %= fact[s.size() - i - 1];
}
]]></content>
